	Journal = {Journal of Geophysical Research: Atmospheres},
	Keywords = {daily maximum and minimum temperatures, diurnal temperature range, Tibetan Plateau},
	Number = {D19},
	Title = {{Temporal trends and variability of daily maximum and minimum, extreme temperature events, and growing season length over the eastern and central Tibetan Plateau during 1961--2003}},
	Volume = {111},
	Year = {2006},
	Bdsk-Url-1 = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2005JD006915},
