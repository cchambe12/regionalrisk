	Keywords = {Budburst,Climate change,Leaf-out,Phenology,Temperate forests},
	Number = {4},
	Pages = {926--941},
	Pmid = {21762163},
	Title = {{Leaf-out phenology of temperate woody plants: From trees to ecosystems}},
