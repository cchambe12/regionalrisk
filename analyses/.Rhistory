plz$end<-ifelse(plz$count=="end", plz$date, NA)
class(plz$end)<-"Date"
plzers<-plz%>%dplyr::select(year, PEP_ID, start, end, lat, long, pepyear)
plzers<-setDT(plzers)[, lapply(.SD, na.omit), by = pepyear]
